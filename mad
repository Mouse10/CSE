import random
def f(x) :answer = random.randint(1, 10)
def a(x) :playing = True
def q(x) :Guesses_left = 5

name = int(input("Guess a number between 1-10: "))
while guesses_left > 0 and playing:
    print
    if name > answer:
        print ("guess lower")
        name = int(input("Guess a number between 1-10: "))
    elif name < answer:
        print ("guess higher")
        name = int(input("Guess a number between 1-10: "))
    else:
        Guesses_left -=1
        print ("You win!!!!")
    print

